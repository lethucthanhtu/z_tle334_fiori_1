<mvc:View
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    controllerName="ztle334fiori1.controller.Strategy"
>
    <Page
        id="_IDGenPage2"
        title="{i18n>StrategyPageTitle}"
    >
        <ObjectHeader
            id="_IDGenObjectHeader"
            title="Strategy"
        />
        <Panel id="_IDGenPanel1">
            <Toolbar id="_IDGenToolbar1">
                <ToolbarSpacer id="_IDGenToolbarSpacer" />
                <Button
                    id="_IDGenButton9"
                    icon="sap-icon://excel-attachment"
                    press="onCSVExport"
                    visible="{= ${viewModel>/editMode}}"
                />
                <Button
                    id="_IDGenButton3"
                    icon="sap-icon://create"
                    press="onCreateEmployee"
                    visible="{= ${viewModel>/editMode}}"
                />
                <Button
                    id="_IDGenButton4"
                    icon="sap-icon://delete"
                    press="onDeleteEmployees"
                    type="Transparent"
                    visible="{= ${viewModel>/editMode} }"
                    enabled="{= ${roleModel>/selectedRole} === 'Admin' }"
                />
                <ToggleButton
                    id="_IDGenToggleButton"
                    icon="{viewModel>/editIcon}"
                    press="onEditToggle"
                />
            </Toolbar>
            <Table
                id="employeeTable"
                items="{employee>/employees}"
                mode="{= ${viewModel>/editMode} ? 'MultiSelect' : 'None' }"
            >
                <columns>
                    <Column id="_IDGenColumn2">
                        <Text
                            id="_IDGenText20"
                            text="Employee ID"
                        />
                    </Column>
                    <Column id="_IDGenColumn10">
                        <Text
                            id="_IDGenText21"
                            text="First Name"
                        />
                    </Column>
                    <Column id="_IDGenColumn11">
                        <Text
                            id="_IDGenText22"
                            text="Last Name"
                        />
                    </Column>
                    <Column id="_IDGenColumn12">
                        <Text
                            id="_IDGenText23"
                            text="Email"
                        />
                    </Column>
                    <Column id="_IDGenColumn13">
                        <Text
                            id="_IDGenText24"
                            text="Phone"
                        />
                    </Column>
                    <Column id="_IDGenColumn14">
                        <Text
                            id="_IDGenText25"
                            text="Manager"
                        />
                    </Column>
                    <Column id="_IDGenColumn15">
                        <Text
                            id="_IDGenText26"
                            text="Subteam"
                        />
                    </Column>
                    <Column
                        id="_IDGenColumn3"
                        visible="{= ${viewModel>/editMode} }"
                    >
                        <Text
                            id="_IDGenText4"
                            text="Actions"
                        />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem id="_IDGenColumnListItem2">
                        <cells>
                            <Text
                                id="_IDGenText27"
                                text="{employee>emp_ID}"
                            />
                            <Text
                                id="_IDGenText28"
                                text="{employee>first_name}"
                            />
                            <Text
                                id="_IDGenText29"
                                text="{employee>last_name}"
                            />
                            <Text
                                id="_IDGenText30"
                                text="{employee>email}"
                            />
                            <Text
                                id="_IDGenText31"
                                text="{employee>phone_no}"
                            />
                            <Text
                                id="_IDGenText32"
                                text="{employee>manager}"
                            />
                            <Text
                                id="_IDGenText33"
                                text="{employee>subteam}"
                            />
                            <HBox
                                id="_IDGenHBox"
                                visible="{viewModel>/editMode}"
                            >
                                <Button
                                    id="_IDGenButton6"
                                    icon="sap-icon://delete"
                                    press="onDeleteEmployee"
                                    type="Transparent"
                                    enabled="{= ${roleModel>/selectedRole} === 'Admin' }"
                                />
                                <Button
                                    id="_IDGenButton5"
                                    icon="sap-icon://edit"
                                    press="onModifyEmployee"
                                    type="Transparent"
                                    enabled="{= ${roleModel>/selectedRole} === 'Admin' }"
                                />
                            </HBox>
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
        </Panel>
    </Page>
</mvc:View>

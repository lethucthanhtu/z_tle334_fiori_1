<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core"
>
  <Dialog
    id="roleDialog1"
    title="Select Role"
    type="Message"
    draggable="true"
    resizable="true"
  >
    <VBox
      width="300px"
      id="_IDGenVBox2"
      class="sapUiSmallMargin"
    >
      <Text id="_IDGenText18" text="Do you want to save changes?"/>
    </VBox>
    <beginButton>
      <Button
        id="_IDGenButton2"
        text="{i18n>Confirm}"
        press="onConfirm"
      />
    </beginButton>
  </Dialog>
</core:FragmentDefinition>
